{
  "kind": "singleType",
  "collectionName": "contact_page",
  "info": {
    "singularName": "contact-page",
    "pluralName": "contact-pages",
    "displayName": "Contact Page",
    "description": "Contact page content"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "subtitle": {
      "type": "text"
    },
    "contactInfoTitle": {
      "type": "string",
      "default": "Contact Information"
    },
    "funFactsTitle": {
      "type": "string",
      "default": "Fun Facts"
    },
    "backgroundImage": {
      "type": "media",
      "multiple": false,
      "allowedTypes": ["images"]
    },
    "funFacts": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::fun-fact.fun-fact"
    }
  }
}

